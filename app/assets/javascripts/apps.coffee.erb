angular.module 'app', [
  'ui.router'
  'ngResource'
  'bgf.paginateAnything'
  'ui.bootstrap'
  'ng-rails-csrf'
  'main'
]
.config ['$urlRouterProvider'
  ($urlRouterProvider) ->
    $urlRouterProvider.otherwise("/")
]
.run ['$state'
    ($state)->
        $state.go('main')
]